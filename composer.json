{
    "name": "szunisoft/laravel-rule-groups",
    "license": "MIT",
    "description": "Make customizable and reusable rule groups easily..",
    "keywords": [
        "laravel",
        "rule",
        "validation",
        "group",
        "reusable"
    ],
    "authors": [
        {
            "name": "Forg√°cs Bence",
            "email": "sykorax93@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "1.1.*",
        "orchestra/testbench": "^3.7"
    },
    "autoload": {
        "files": [
        ],
        "psr-4": {
            "SzuniSoft\\RuleGroups\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SzuniSoft\\RuleGroups\\Test\\": "tests/"
        }
    },
    "suggest": {
        "illuminate/validation": "5.5.* | 5.6.*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "aliases": {
            },
            "providers": [
                "SzuniSoft\\RuleGroups\\Providers\\RuleGroupServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    }
}
